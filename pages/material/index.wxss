.material-item {
  height: 160rpx;
  display: flex;
  align-items: center;
}

.suffix {
  width: 80rpx;
  height: 80rpx;
}

.content {
  width: calc(100% - 80rpx - 100rpx);
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  padding-left: 32rpx;
}

.operation{
  width: 100rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.name{
  margin-bottom: 16rpx;
}

.date{
  font-size: 24rpx;
}

.download{
  font-size: 28rpx;
}



